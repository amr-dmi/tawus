<html xmlns:t='http://tapestry.apache.org/schema/tapestry_5_1_0.xsd'>

   <body>
      <div t:type='zone' t:id='outerZone' id='outerZone'>
         <div t:type='tawus/tabpanel' t:id='outerTab' t:zone='outerZone' t:tabs='tabA, tabB,tabC'>

            <div t:type='tawus/tab' title='Tab A' t:id='tabA'>Content of Tab A</div>

            <div t:type='tawus/tab' title='Tab B' t:id='tabB'>Content of Tab B</div>

            <div t:type='tawus/tab' title='Tab C' t:id='tabC'>
            <br/><br/>
               <div style='padding:5px 15px' t:type='zone' t:id='innerZone' id='innerZone'>
                  <div t:type='tawus/tabpanel' t:id='innerTab' t:zone='innerZone' t:tabs='tabX, tabY'>

                     <div t:type='tawus/tab' title='Tab X' t:id='tabX'>Content of Tab X</div>

                     <div t:type='tawus/tab' title='Tab Y' t:id='tabY'>Content of Tab Y</div>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </body>
</html>